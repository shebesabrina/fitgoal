<script type="text/javascript">
  $(function() {
    const calendar = $('#calendar');
    calendar.fullCalendar({
      header: {
        left: 'prev,next,today',
        center: 'title',
        right: 'month,agendaWeek,agendaDay'
        }
    })
  });
</script>
